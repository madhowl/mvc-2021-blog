<nav class="nav-menu d-none d-lg-block">

    <ul>
        {% block nav %}
            {% for link in links %}
                <li {% if link.active == true %} class="active" {% endif %}>
                    <a href="{{ link.href }}">{{link.title}}</a></li>
            {% endfor %}
        {% endblock %}
    </ul>
</nav>